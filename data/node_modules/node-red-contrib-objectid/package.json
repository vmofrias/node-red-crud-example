{
  "name": "node-red-contrib-objectid",
  "version": "1.1.0",
  "description": "Node-RED node to create an Object-Id for mongodb operations.",
  "keywords": [
    "node-red",
    "database",
    "object-id",
    "mongodb",
    "biancoroyal"
  ],
  "author": "Klaus Landsdorf",
  "license": "MIT",
  "homepage": "https://noderedplus.de/",
  "dependencies": {
    "bson": "^4.6.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BiancoRoyal/node-red-contrib-objectid"
  },
  "bugs": {
    "url": "https://github.com/BiancoRoyal/node-red-contrib-objectid/issues/"
  },
  "engines": {
    "node": ">=12"
  },
  "main": "node",
  "node-red": {
    "version": ">=2",
    "nodes": {
      "objectid": "objectid.js"
    }
  },
  "scripts": {
    "test": "standard --fix",
    "build": "standard --fix",
    "prepublishOnly": "standard --fix && npm test && npm run build",
    "ci-publish": "ci-publish",
    "release": "standard-version -a",
    "rewrite-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "dev-link": "npm i && npm run build && npm link",
    "dev-unlink": "npm unlink node-red-contrib-objectid -g"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^2.2.2",
    "standard": "^17.0.0",
    "standard-version": "^9.5.0"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "standard": {
    "ignore": [
      "node_modules/",
      "examples/",
      "docs",
      "test",
      "extras"
    ]
  }
}
